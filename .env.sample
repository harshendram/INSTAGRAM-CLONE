# MongoDB Atlas Connection String
# Replace username, password, and clustername with your actual values
# Make sure to whitelist your IP address in MongoDB Atlas
MONGODB_URI=mongodb+srv://<username>:<password>@<clustername>.mongodb.net/instaclone?retryWrites=true&w=majority

# JWT Secret Key
SECRET_KEY=your_jwt_secret_key_here

# Server Port
PORT=5000

# Frontend URL for CORS
URL=http://localhost:5173

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
